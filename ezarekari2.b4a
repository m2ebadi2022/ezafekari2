Sub chek_time_backup
	

	
	If(File.Exists(File.DirInternal,"chk_backup.txt")=False)Then
		File.WriteString(File.DirInternal,"chk_backup.txt",myfunc.fa2en(DateTime.Date(DateTime.Now)))
	Else 
			
		Dim tim1_ck_backup As String=File.ReadString(File.DirInternal,"chk_backup.txt")
		Dim tim2_ck_backup As String=myfunc.fa2en(DateTime.Date(DateTime.Now))
		Dim time_between As Long = persianDate.CalculateDaysBetween(tim1_ck_backup,tim2_ck_backup)
		
		Dim days_for_backup As Int
		Dim res_backup As Int = dbCode.get_setting_byName("backup_online")
		Select res_backup
			Case 0
				days_for_backup=7
			Case 1
				days_for_backup=14
			Case 2
				days_for_backup=30
			Case 3
				days_for_backup=-1
		End Select
		
		'test
		'is_time_backup=True
		
		If(days_for_backup<0)Then
			is_time_backup=False
		else If(time_between>= days_for_backup)Then
			is_time_backup=True
		End If
		
	End If
	
	
	
End Sub

