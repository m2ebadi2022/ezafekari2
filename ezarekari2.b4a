						If (state_tatil=1 Or state_tatil=2) Then 'tatil rasmi 'tatil garardadi
							add_taradod_sub2
						Else
							add_taradod_sub
						End If
						
					Case 11
						
						If (state_tatil=1) Then 'tatil rasmi 
							add_taradod_sub2
						Else
							add_taradod_sub
						End If
						
					Case 22
						
						If (state_tatil=2) Then  'tatil garardadi
							add_taradod_sub2
						Else
							add_taradod_sub
						End If
						
				End Select
				
				
				'eeeeeeeeeee
				If(is_for_edit=False)Then
					lbl_rest_finger_fs_taradod_Click
				End If
				
			Else
				lbl_vip_Click
			End If
		Else
			
			Select state_tatil_setting
				Case 0
					add_taradod_sub
				Case 1
					If (state_tatil=1 Or state_tatil=2) Then 'tatil rasmi 'tatil garardadi
						add_taradod_sub2
					Else
						add_taradod_sub
					End If
						
				Case 11
						
					If (state_tatil=1) Then 'tatil rasmi
						add_taradod_sub2
					Else
						add_taradod_sub
					End If
						
				Case 22
						
					If (state_tatil=2) Then  'tatil garardadi
						add_taradod_sub2
					Else
						add_taradod_sub
					End If
						
			End Select
			
			
			
			If(is_for_edit=False)Then
				lbl_rest_finger_fs_taradod_Click
			End If
			
