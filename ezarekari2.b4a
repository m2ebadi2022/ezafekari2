Sub Activity_Create(FirstTime As Boolean)
	

	''----------------------------
	Try
		inapp.Initialize("inapp",key)
	Catch
		Log(LastException)
	End Try
	
	If(FirstTime=True)Then
		dbCode.check_old_adds
		dbCode.check_new_add
	End If
	
	
	
	''---------------------------------
	
	
	img1.Initialize(LoadBitmap(File.DirAssets,"m1.png"))
	img1.Gravity=Gravity.FILL
	
	img2.Initialize(LoadBitmap(File.DirAssets,"m2.png"))
	img2.Gravity=Gravity.FILL
	
	img3.Initialize(LoadBitmap(File.DirAssets,"m3.png"))
	img3.Gravity=Gravity.FILL
	
	If(FirstTime)Then
		Activity.LoadLayout("splash_layout")
		lbl_vesion_app.Text=app_vesion
		cv_loader.Show
		timer1.Initialize("timer1" , 1500)
		
		timer1.Enabled=True
		is_first_time=True
	Else
		Activity.LoadLayout("main_layout")
		btn_menu_home_Click
		is_first_time=False
		
	End If

	''  for date picker
	moon.Initialize
	moon.AddAll(Array As String("فروردین", "اردیبهشت","خرداد", "تیر","مرداد", "شهریور","مهر", "آبان","آذر", "دی","بهمن", "اسفند"))
	''-----------------
	
	strfun.Initialize
	
	
	
	If(buy_index=1)Then
		lbl_vip_Click
		buy_index=0
	End If
	
	
	
	
	tim_msg.Initialize("tim_msg",4000)
	
	ls1.Initialize
	ls2.Initialize
	ls3.Initialize
	
	
	If(File.Exists(File.DirInternal,"ls1")) Then
		ls1=File.ReadList(File.DirInternal,"ls1")
		ls2=File.ReadList(File.DirInternal,"ls2")
		ls3=File.ReadList(File.DirInternal,"ls3")
		
		last_notif=ls1.Get(ls1.Size-1)
		is_now_instal=False
	End If
	
	

	
	If(FirstTime=True)Then
		If(myfunc.check_karid=True)Then
			http_initial_1(2)
		Else
			http_initial_1(1)
		End If
	
	End If
	
	''----------- msg new year
'	tim_msg_newYear.Initialize("tim_msg_newYear" , 2000)
'	
'	If (File.Exists(File.DirInternal,"new_msg2")=True)Then
'		
'	Else
'		
'		File.WriteString(File.DirInternal,"new_msg2","")
'		
'		tim_msg_newYear.Enabled=True
'		
'	End If
'	
	
	If(File.Exists(File.DirInternal,"phonNum"))Then
		phon_num=File.ReadString(File.DirInternal,"phonNum")
	End If
	
	If(File.Exists(File.DirInternal,"userAcc"))Then
		Dim ls_user As List
		ls_user.Initialize
		ls_user=File.ReadList(File.DirInternal,"userAcc")
		user_nameFamili=ls_user.Get(0)
	End If
	
	
	If(FirstTime=True)Then
		tim_check_update.Initialize("tim_check_update",9000)
		tim_check_update.Enabled=True
	End If
	
End Sub
Sub tim_check_update_Tick
	chk_update_auto
	Log("now cheking ....")
