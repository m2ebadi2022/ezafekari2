Sub what_day (year1 As Int,moon1 As Int, day1 As Int) As Int
	Dim id_MyDay As Int
	id_MyDay=dbCode.get_day_id(year1,moon1,day1)
	
	dbCode.connect_db
	dbCode.res =  dbCode.sql.ExecQuery("SELECT * FROM 'my_calander' WHERE id="&id_MyDay)
	
	dbCode.res.Position = 0
	Dim result_int As Int=dbCode.res.GetInt("day_h")
	' 1 is shanbe
	' 7 is jome
	
	Select result_int
		Case 1
			day_inWeek_name="شنبه"
		Case 2
			day_inWeek_name="یکشنبه"
		Case 3
			day_inWeek_name="دوشنبه"
		Case 4
			day_inWeek_name="سه شنبه"
		Case 5
			day_inWeek_name="چهارشنبه"
		Case 6
			day_inWeek_name="پنجشنبه"
		Case 7
			day_inWeek_name="جمعه"
				
	End Select
	
	Return result_int
End Sub




Sub chek_tatil_day (year1 As Int,moon1 As Int, day1 As Int) As Int
	
	Dim id_thisDay As Int
	id_thisDay=dbCode.get_day_id(year1,moon1,day1)
	
	dbCode.connect_db
	dbCode.res =  dbCode.sql.ExecQuery("SELECT * FROM 'my_calander' WHERE id="&id_thisDay)
	
	dbCode.res.Position = 0
	
	
	Dim result_int As Int=0
	If (dbCode.res.GetString("state") = "tatil")Then
		result_int=1
	else If (dbCode.res.GetString("state") = "tatil1")Then
		result_int=2
	End If
	
'	If(dbCode.res.GetInt("day_h")=7)Then
'		result_int=3
'	End If
	
	Return result_int    ' result_int=0 is not tatil   & 1 => tatil rasmi  &  tatil1 => tatil garardadi  & 3 => jome
	
End Sub

Sub chek_tatil_setting As Int
	Dim temp As Int=0
	Dim sett_a ,sett_b  As Int=0
	
	sett_a=dbCode.get_setting_byName("setting_tatil_rasmi")
	sett_b=dbCode.get_setting_byName("setting_tatil_garardadi")
	
	
	If(sett_a=1)Then
		temp=11
		If (sett_b=1)Then
			temp=1
		End If
		
	Else
		temp=0
		If (sett_b=1)Then
			temp=22
		End If
	End If
	
	
	Return temp
	
End Sub

